{
  "schemaVersion": 1,
  "id": "tiptapshow",
  "version": "${version}",
  "name": "TipTapShow",
  "description": "A mod to show your keystrokes ingame",
  "authors": [
    "Spyxar"
  ],
  "contact": {
    "homepage": "https://github.com/Spyxar",
    "sources": "https://github.com/Spyxar/TipTapShow",
    "issues": "https://github.com/Spyxar/TipTapShow/issues"
  },
  "license": "MIT",
  "icon": "assets/tiptapshow/icon.png",
